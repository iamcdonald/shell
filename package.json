{
  "name": "learnz",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged && yarn lint && yarn coverage",
    "lint": "eslint '{src,test}/**/*.js'",
    "start:dev": "webpack-dev-server --hot --inline --host 0.0.0.0 --port 8080 --no-info",
    "build": "rm -rf public && webpack",
    "unit-test": "ava 'test/**/*.spec.js'",
    "coverage": "nyc npm run unit-test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-semistandard-react": "^3.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.3",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.2",
    "proxyquire": "^1.8.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "ava": {
    "require": [
      "./test/unit/css-modules-compiler",
      "babel-register"
    ],
    "babel": "inherit"
  }
}
