{
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "test/**/*.spec.js",
    "public/**/*.js"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "require": [
    "babel-register"
  ],
  "cache": false,
  "all": true,
  "check-coverage": true
}
